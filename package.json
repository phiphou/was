{
  "name": "WAS",
  "version": "0.0.1",
  "description": "Webpack Angular 2 Sample App",
  "license": "MIT",
  "homepage": "https://github.com/phiphou/was#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/phiphou/was"
