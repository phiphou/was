  "private": true,
  "engines": {
    "node": ">= 5.10.1",
    "npm": ">= 3.9.5"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "karma  start ./config/karma.conf.js",
    "build": "rimraf dist && cross-env NODE_ENV=production node ./config/webpack/wp",
    "build-gz": "rimraf dist && cross-env NODE_ENV=production node ./config/wp --gz",
    "watch": "cross-env NODE_ENV=development && webpack --watch --progress --profile",
    "start": "webpack-dev-server --config ./config/webpack/webpack.dev.js --inline --progress --port 8080",
    "tests": "cd config && test.sh",
    "postinstall": "typings install",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "e2e": "protractor ./config/protractor.conf.js",
    "lint": "tslint src/**/*.ts",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular2-material/card": "^2.0.0-alpha.6-2",
    "@angular2-material/core": "2.0.0-alpha.8-2",
    "@angular2-material/input": "2.0.0-alpha.8-2",
    "@angular2-material/slide-toggle": "2.0.0-alpha.8-2",
    "es6-shim": "^0.35.1",
    "font-awesome": "^4.6.3",
    "foundation-sites": "^6.2.3",
    "ng2-translate": "^5.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.2",
    "weather-icons": "https://github.com/erikflowers/weather-icons.git",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
