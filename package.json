    "jasmine": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^4.1.1",
    "null-loader": "^0.1.1",
    "ora": "^0.4.0",
    "postcss-loader": "^1.2.1",
    "progress-bar-webpack-plugin": "^1.9.0",
    "protractor": "^4.0.2",
    "raw-loader": "^0.5.1",
