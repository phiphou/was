  "private": true,
  "engines": {
    "node": ">= 5.10.1",
    "npm": ">= 3.9.5"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "karma  start ./config/karma.conf.js",
    "build": "rimraf dist && cross-env NODE_ENV=production node ./config/webpack/wp",
    "build-gz": "rimraf dist && cross-env NODE_ENV=production node ./config/wp --gz",
    "watch": "cross-env NODE_ENV=development && webpack --watch --progress --profile",
    "start": "webpack-dev-server --config ./config/webpack/webpack.dev.js --inline --progress --port 8080",
    "tests": "cd config && test.sh",
    "postinstall": "typings install",
    "server": "webpack-dev-server --inline --progress --port 8080",
    "e2e": "protractor ./config/protractor.conf.js",
    "lint": "tslint src/**/*.ts",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
